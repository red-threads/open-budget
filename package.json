{
  "name": "@red-threads/open-budget-api",
  "version": "0.0.1",
  "description": "Open Budget API",
  "homepage": "https://github.com/red-threads/open-budget-api",
  "issues": "https://github.com/red-threads/open-budget-api/issues",
  "main": "src/index.js",
  "engines": {
    "node": "10.x.x",
    "npm": "6.x.x"
  },
  "scripts": {
    "jest": "NODE_ENV=test jest",
    "lint": "standard --fix",
    "test": "run-p lint jest",
    "start": "node .",
    "dev": "nodemon --inspect",
    "push-tags": "git push --follow-tags origin master",
    "deploy": "now --public",
    "prerelease": "npm test",
    "release": "standard-version",
    "postrelease": "run-p push-tags deploy"
  },
  "keywords": [
    "red-threads",
    "openbudget",
    "collective",
    "nonprofit"
  ],
  "author": "Red Threads",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "csv-parse": "^2.5.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "fastify": "^1.5.0",
    "fastify-mongodb": "^0.7.0",
    "json-api": "^3.0.0-rc.4.0.1",
    "mongoose": "^5.1.4",
    "next": "^6.0.3",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "pluralize": "^7.0.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "standard": "^11.0.1",
    "standard-version": "^4.4.0",
    "timm": "^1.4.2"
  },
  "env": {
    "MONGO_USER": "@mongo-user",
    "MONGO_PASSWORD": "@mongo-password",
    "MONGO_SERVER": "@mongo-server"
  }
}
